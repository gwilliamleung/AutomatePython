import re, pyperclip

websiteRegex = re.compile(r'''(
    https?://
    (?:w{3}\.)?
    [a-zA-Z0-9.-]+
    \.
    [a-zA-Z]
    .
    ([a-zA-Z0-9._%+-/])+
    /
    [a-zA-Z]+
    )''',re.VERBOSE
)

text = str(pyperclip.paste())

matches = []
for groups in websiteRegex.findall(text):
    matches.append(groups[0])

if len(matches)>0:
    print('Copied to clipboard:')
    print('\n'.join(matches))
else:
    print('No websites found')